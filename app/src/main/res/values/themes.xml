<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Fauna" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <!-- <item name="colorPrimary">@color/purple_500</item> -->
        <item name="colorPrimary">@color/white_secondary</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorOnPrimary</item>
        <item name="android:windowLightStatusBar" tools:targetApi="23">true</item>
        <!-- Customize your theme here. -->
        <item name="colorBgSecondary">@color/white_secondary</item>
        <item name="colorStroke">@color/white_primary</item>
        <item name="colorBgInformation">@color/light_yellow</item>
        <item name="colorGray">@color/gray</item>

    </style>


</resources>